{
    "name": "ai-gist",
    "version": "1.3.3",
    "description": "本地优先的 AI 提示词管理工具，能够管理 AI 提示词 + 变量填充 + 分类标签。",
    "main": "./main/main.js",
    "scripts": {
        "dev": "node scripts/dev-server.js",
        "build": "node scripts/build.js && electron-builder",
        "build:win": "node scripts/build.js && electron-builder --win",
        "build:mac": "node scripts/build.js && PYTHON=python3 npm_config_python=python3 electron-builder --mac",
        "build:linux": "node scripts/build.js && electron-builder --linux",
        "build:ci:mac": "node scripts/build.js && CSC_IDENTITY_AUTO_DISCOVERY=false yarn electron-builder --mac",
        "build:ci:win": "node scripts/build.js && yarn electron-builder --win",
        "build:ci:linux": "node scripts/build.js && yarn electron-builder --linux",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "test": "vitest",
        "test:run": "vitest run",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage",
        "version": "git add -A",
        "postversion": "git push && git push --tags"
    },
    "repository": "https://github.com/yarin-zhang/ai-gist",
    "author": {
        "name": "Yarin Zhang",
        "url": "https://github.com/yarin-zhang"
    },
    "devDependencies": {
        "@eslint/js": "^9.29.0",
        "@types/nunjucks": "^3.2.6",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.35.0",
        "@typescript-eslint/parser": "^8.35.0",
        "@vitejs/plugin-vue": "^4.4.1",
        "@vitest/coverage-v8": "^3.2.3",
        "@vitest/ui": "^1.6.0",
        "@vue/eslint-config-typescript": "^14.5.1",
        "@vue/tsconfig": "^0.7.0",
        "chalk": "^4.1.2",
        "chokidar": "^4.0.3",
        "electron": "^32.1.2",
        "electron-builder": "^25.1.6",
        "eslint": "^9.29.0",
        "eslint-plugin-vue": "^10.2.0",
        "jsdom": "^24.1.0",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.0",
        "vite": "^6.3.5",
        "vitest": "^3.2.3",
        "vue-eslint-parser": "^10.1.4"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.53.0",
        "@google/generative-ai": "^0.24.1",
        "@langchain/anthropic": "^0.3.21",
        "@langchain/core": "^0.3.57",
        "@langchain/google-genai": "^0.2.10",
        "@langchain/ollama": "^0.2.1",
        "@langchain/openai": "^0.5.12",
        "@vicons/tabler": "^0.13.0",
        "archiver": "^7.0.1",
        "langchain": "^0.3.27",
        "naive-ui": "^2.41.0",
        "nunjucks": "^3.2.4",
        "sass": "^1.89.2",
        "tailwindcss": "^4.1.11",
        "unzipper": "^0.12.3",
        "uuid": "^11.1.0",
        "vue": "^3.3.8",
        "vue-i18n": "9",
        "webdav": "^5.5.0",
        "zod": "^3.25.40"
    },
    "packageManager": "yarn@4.9.2"
}
